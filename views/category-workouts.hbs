<div class="row">
    <div class="col-md-3">
        <div class="list-group list-group-custom">
            <a href="/dashboard" class="list-group-item list-group-item-action">Dashboard</a>
            <a href="/workouts" class="list-group-item list-group-item-action active">Workouts</a>
            <a href="/diary" class="list-group-item list-group-item-action">Workout Diary</a>
        </div>
    </div>
    
    <div class="col-md-9" style="color: white">
        <h2>{{category}} Workouts</h2>
        <div class="row">
            {{#each workouts}}
                {{#each exercises}}
                    <div class="col-md-6">
                        <div class="card mb-3 bg-dark text-center">
                            <a href="/workouts/{{this}}">
                                <div class="card-body card-body-custom" style="background-image: url('assets/img/{{category}}.jpg'); background-size: cover; background-position: center; color: white;">
                                </div>
                                <h5 class="card-title" style="color: white">{{this}}</h5>
                            </a>
                        </div>
                        </a>
                    </div>
                {{/each}}
            {{/each}}
        </div>
    </div>
</div>